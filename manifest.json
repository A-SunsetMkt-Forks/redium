{
  "manifest_version": 3,
  "name": "Redium",
  "version": "1.0",
  "description": "Automatically unblock articles through proxies like 12ft, LibMedium, and Scribe",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "action": {},
  "host_permissions": ["*://*.medium.com/*"],
  "permissions": [
    "declarativeNetRequestWithHostAccess",
    "activeTab",
    "contextMenus",
    "storage",
    "management"
  ],
  "background": {
    "service_worker": "src/worker.js"
  }
}
